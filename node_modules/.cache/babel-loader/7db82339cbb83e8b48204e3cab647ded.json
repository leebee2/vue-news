{"ast":null,"code":"import ListItem from '../components/ListItem.vue';\nexport default {\n  components: {\n    ListItem\n  }\n};","map":{"version":3,"mappings":"AA+BA;AAEA;EACAA;IACAC;EADA;AADA","names":["components","ListItem"],"sourceRoot":"src/views","sources":["ListView.vue"],"sourcesContent":["<template>\n  <div>\n    <ul class=\"list\">\n      <li v-for=\"(item, index) in listItem\" :key=\"index\" :class=\"[{ 'addpost': $route.name !== 'news' }, 'post']\">\n        <div v-show=\"$route.name === 'news'\" class=\"points\">\n          {{ item.points || 0 }}\n        </div>\n        <div>\n          <p class=\"title\">\n            <template v-if=\"item.domain\">\n              <a :href=\"item.url\" target='_blank'>{{ item.title }}</a>\n            </template>\n            <template v-else>\n              <router-link :to=\"`/item/${item.id}`\">\n                {{ item.title }}\n              </router-link>\n            </template>\n          </p>\n          <small class=\"link-text\">\n            {{ item.time_ago }} by\n            <router-link :to=\"`/user/${item.user}`\" class=\"link-text\" v-if=\"item.user\">{{ item.user }}\n            </router-link>\n            <a :href=\"item.url\" target='_blank' v-else>{{ item.domain }}</a>\n          </small>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport ListItem from '../components/ListItem.vue';\n\nexport default {\n  components: {\n    ListItem,\n  },\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}